<% include ../partials/header %>


   <div class="ui main text container" style ="margin:150px 125px 25px 125px">
   		<div class="ui top attached segment">
   			<div class="ui divided items">
   				<% feeds.forEach(function(items){ %>
            
              <div class="item">
              <div class = "content">
               <h2><%= items.username %> </h2>
                
                <div class="meta">
                  <h2><%= items.reported %></h2>
                </div>  
                <div class="description">
              
                </div>
                <div>
                 </div>
                   <div class="extra" >
                       
                       <a class="ui floated basic red button pull-left" href="/sendMessage/<%=items.username%>">Send
                       <i class="right chevron icon"></i>
                        </a>
                        <a class="ui floated basic red button pull-right" href="/remove/<%=items.username%>">Remove
                       <i class="right chevron icon"></i>
                        </a>
                     </div>
              </div>
            </div>

            
   					
   				<%}) %>
   			</div>
   		</div>
   </div>



<% include ../partials/footer %>