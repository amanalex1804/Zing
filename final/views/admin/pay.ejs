<% include ../partials/header %>


   <div class="ui main text container" style ="margin:150px 125px 25px 125px">
   		<div class="ui top attached segment">
   			<div class="ui divided items">
   				<% feeds.forEach(function(items){ %>
            
              <div class="item">
              <div class = "content">
             
                <h3><%= items.username%></h3>
                <div class="meta">
                  <h3><%= items.money %></h3>
              
                </div>  
                <div class="description">
              
                </div>
                <div>
                 </div>
                   <div class="extra" >
                       
                       <a class="ui floated basic red button pull-right" href="/pays/<%=items.username%>/<%=items.money%>" target="_blank">Pay
                       <i class="right chevron icon"></i>
                        </a>
                     </div>
              </div>
            </div>

            
   					
   				<%}) %>
   			</div>
   		</div>
   </div>



<% include ../partials/footer %>